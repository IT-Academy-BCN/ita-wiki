{
  "name": "service_wiki",
  "private": true,
  "version": "0.16.1",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm src/server.ts",
    "build": "tsc",
    "bundle": "ncc build src/server.ts -o dist -m",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "test": "vitest --no-threads",
    "up": "cd ../../scripts && ./up_wiki.sh && cd ../services/wiki && npm run prisma:migrate:reset",
    "start:prod": "prisma migrate deploy && node dist/server.js"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/koa": "2.13.5",
    "@types/koa__cors": "4.0.0",
    "@types/koa__multer": "^2.0.4",
    "@types/koa__router": "12.0.0",
    "@types/node": "18.15.5",
    "@types/qs": "^6.9.7",
    "@types/supertest": "2.0.12",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vercel/ncc": "0.36.1",
    "@vitest/coverage-c8": "0.30.1",
    "eslint": "8.36.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "msw": "^2.0.10",
    "node-html-parser": "6.1.5",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "prisma": "^4.12.0",
    "supertest": "6.3.3",
    "tsx": "^3.12.6",
    "typescript": "5.0.2",
    "vitest": "0.30.1"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "4.5.0",
    "@koa/cors": "5.0.0",
    "@koa/multer": "^3.0.2",
    "@koa/router": "12.0.0",
    "@prisma/client": "^4.12.0",
    "bcrypt": "5.1.0",
    "dotenv": "16.0.3",
    "jsonwebtoken": "^9.0.0",
    "koa": "2.14.1",
    "koa-body": "6.0.1",
    "koa-helmet": "7.0.1",
    "koa2-swagger-ui": "^5.7.0",
    "multer": "^1.4.5-lts.1",
    "qs": "^6.11.2",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.1",
    "yaml": "^2.2.2",
    "yamljs": "^0.3.0",
    "zod": "3.22.3"
  }
}
